<div class="col-lg-9">
<table class="table table-primary">
    <tr>
        <th colspan="7"  style="margin-left: 50px;">Book list</th>
    </tr>
    <tr>
        <th>Title</th>
        <th>Author</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Date</th>
        <th>Image</th>
        <th>Video</th>
    </tr>
    {{#each books }}
        <tr>
            <th>{{title}}</th>
            <th>{{author}}</th>
            <th>{{price}}$</th>
            <th>{{quantity}}</th>
            <th>{{ dateFormat date "DD/MM/YYYY" }}</th>
            <th><img src="{{image}}" width="100" height="100"></th>
            <th><iframe src="https://www.youtube.com/embed/{{video}}" frameborder="0"></iframe></th>
        </tr>  
    {{/each}}
</table>
</div>